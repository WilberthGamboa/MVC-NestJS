<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>App</title>
  </head>
  <body>
    <h1>Mis computadoras</h1>
    {{#each data}}
          {{#if nuevaImagen}}
         <img src={{nuevaImagen}} alt="" width="400px" height="400px">
          {{/if}}
          {{/each}}

    {{> pcPartial}}
    
  <div>
    <a href="http://localhost:3000/myPc?offset={{pagination.previousPage}}"> 

      <input type="button" value="{{pagination.previousPage}}">

    </a>
    <p>
      {{pagination.currentPage}}
    </p>
    <a href="http://localhost:3000/myPc?offset={{pagination.nextPage}}"> 

      <input type="button" value="{{pagination.nextPage}}">

    </a>


  </div>
  </body>
</html>